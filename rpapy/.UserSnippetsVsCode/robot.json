	"Robot Get Element Vision":{
		"prefix": "Get Element Vision",
		"body": [
			"Get Element Vision    ${1:image_name}    backend=${2|uia,win32|}    max_wait=${3:30.0}",
			"...    execute=${4:False}    args=${5:$${{ [] }}    kwargs=${6:$${{ {} }}",
			"...    attr_name=${7|click_input,click,double_click_input,select,set_focus,type_keys|}     wait_attr=${8:5.0}",
			"...    delay=${9:0.0}    before=${10:0.0}   after=${11:0.0}",
			"...    move_x=${12:0}    move_y=${13:0}    ignore_error=${14:${False}}",
