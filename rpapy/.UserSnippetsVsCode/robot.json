{
	// Place your snippets for robot here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	
	"Robot Click Vision":{
		"prefix": "Click Vision",
		"body": [
			"Click Vision    ${1:image_name}    backend=${2|uia,win32|}    max_wait=${3:30.0}",				
			"...    button=${4|left,right|}    delay=${5:0.0}    before=${6:0.0}   after=${7:0.0}",
			"...    wait_attr=${8:5.0}     move_x=${9:0}    move_y=${10:0}    ignore_error=${11:${False}}",
			"$0"
		],
		"description": "Keyword to click in the element UI"
	},
	
	"Robot Click Coord":{
		"prefix": "Click Coord",
		"body": [
			"Click Coord    ${1:x}    ${2:y}    backend=${3|uia,win32|}",				
			"...    button=${4|left,right|}    delay=${5:0.0}    before=${6:0.0}   after=${7:0.0}",
			"...    wait_attr=${8:5.0}",
			"$0"
		],
		"description": "Keyword to click in the element UI by coordinate",
	},


